            <Font size="15.0" />
         </font>
      </Label>
      <ImageView fx:id="closeButton" fitHeight="32.0" fitWidth="32.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#closeThisGoMain" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/closebutton2.png" />
