         <font>
            <Font name="AppleGothic Regular" size="16.0" />
         </font>
      </Label>
      <Label alignment="TOP_LEFT" layoutX="78.0" layoutY="251.0" prefHeight="30.0" prefWidth="112.0" text="Amount :" textFill="WHITE" wrapText="true">
         <font>
            <Font name="AppleGothic Regular" size="22.0" />
         </font>
      </Label>
      <TextField fx:id="amountTxt" layoutX="217.0" layoutY="253.0" prefHeight="26.0" prefWidth="196.0" />
      <TextField fx:id="tarAccNumTxt" layoutX="217.0" layoutY="293.0" prefHeight="26.0" prefWidth="196.0" />
      <Label alignment="TOP_CENTER" layoutX="125.0" layoutY="14.0" prefHeight="65.0" prefWidth="222.0" text="Transfer" textAlignment="CENTER" textFill="WHITE">
